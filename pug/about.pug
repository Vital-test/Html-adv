mixin about
  - var items = [{src:'../static/img/icon-feature-1.svg', text:'Экологически чистые материалы'},{src:'../static/img/icon-feature-2.svg', text:'Скандинавский стиль по российской цене'}, {src:'../static/img/icon-feature-3.svg', text:'Увеличивает лайки на фотографиях'},{src:'../static/img/icon-feature-4.svg', text:'Связано вручную с любовью и умилением'},{src:'../static/img/icon-feature-5.svg', text:'Поддержка отечественного производителя'},{src:'../static/img/icon-feature-6.svg', text:'Поставляется в подарочной упаковке'}];
  .about
    .content
      H2.about__title Коротко о нас:
      UL.about__list
        each rows in items
          LI.about__item
            IMG(src=rows.src)
            = rows.text